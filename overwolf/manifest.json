{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Overwolf Next Boilerplate App",
    "version": "1.0.0",
    "minimum-overwolf-version": "0.77.10",
    "author": "Developer Name",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png",
    "description": "Overwolf Next Boilerplate App"
  },
  "data": {
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "index.html",
        "transparent": false,
        "resizable": true,
        "use_os_windowing": true,
        "size": {
          "width": 700,
          "height": 400
        },
        "min_size": {
          "width": 400,
          "height": 400
        }
      },
      "OverlayWindow": {
        "file": "overlay.html",
        "transparent": true,
        "resizable": false,
        "size": {
          "width": 400,
          "height": 300
        },
        "in_game_only": true
      }
    }
  }
}
